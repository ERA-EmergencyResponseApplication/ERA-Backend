      "type": "belongsTo",
      "model": "Emergency",
      "foreignKey": ""
    },
    "responder": {
      "type": "belongsTo",
      "model": "Responder",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
