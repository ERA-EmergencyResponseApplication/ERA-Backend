{
  "name": "EmergencyResponse",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
